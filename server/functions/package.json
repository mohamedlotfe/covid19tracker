    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "emulator": "firebase init emulators"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "apify-client": "^2.8.4",
    "axios": "^1.6.7",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.3.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0",
    "typescript": "^4.9.0"
  },
  "private": true
}
